      <memberdef kind="function" id="unistd_8h_1a116146ccc01b751779e5351615743547" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type><ref refid="types_8h_1a1bd78dbcdb69d819cb6ea4dfa78daaec" kindref="member">ssize_t</ref></type>
        <definition>ssize_t read</definition>
        <argsstring>(int fileds, void *buffer, size_t count)</argsstring>
        <name>read</name>
        <param>
          <type>int</type>
          <declname>fileds</declname>
        </param>
        <param>
          <type>void *</type>
